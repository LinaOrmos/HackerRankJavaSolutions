//What is the difference

Public class ExampleVariable{
public static void main(string[] args){
float variable=3.5
#error while declaring the variable
}
}
// So we ad an f at the end
Public class ExampleVariable{
public static void main(string[] args){
float variable=3.5f
    System.out.println(variable);
}
}
//we can make operations also
Public class ExampleVariable{
public static void main(string[] args){
float variable=3.5e2f
    System.out.println(variable);
}
}
//now we print the double variable we create
Public class ExampleVariable{
public static void main(string[] args){
float variable=3.5fe2f
    System.out.println(variable);
double variable2=4.87
System.out.println(variable2);
}
}
